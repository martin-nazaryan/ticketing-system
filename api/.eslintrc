{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
        "import/prefer-default-export": 0,
        "class-methods-use-this": 0,
        "import/no-named-as-default-member": 0,
        "import/no-named-as-default": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "prefer-promise-reject-errors": 0,
        "camelcase": 1
    }
}
